#include <Wire.h>
#include <LiquidCrystal_I2C.h>
LiquidCrystal_I2C lcd(0x27, 16, 2);
void setup() {
  // put your setup code here, to run once:
lcd.begin(16, 2);
  lcd.init();
  lcd.backlight();
}

void loop() {
  // put your main code here, to run repeatedly:
  int j=23;
lcd.setCursor(0, 1);
  lcd.print("hello:");
  lcd.print(j);
  lcd.setCursor(10, 1);
  lcd.print("hello");
  delay(1000);
  
  lcd.clear();
  delay(1000);
lcd.setCursor(0, 1);
  lcd.print("baby");
  lcd.setCursor(10, 1);
  lcd.print("sexy");
  delay(1000);

}
